<launch>
    <group ns="motors">
    <node pkg="roboteq_driver" type="driver" name="front_right" output="screen">
        <rosparam>
            port: /dev/roboteq/front-right
            baud: 115200
        </rosparam>
    </node>
    <node pkg="roboteq_driver" type="driver" name="front_left" output="screen">
        <rosparam>
            port: /dev/roboteq/front-left
            baud: 115200
        </rosparam>
    </node>
    <node pkg="roboteq_driver" type="driver" name="rear_right" output="screen">
        <rosparam>
            port: /dev/roboteq/rear-right
            baud: 115200
        </rosparam>
    </node>
    <node pkg="roboteq_driver" type="driver" name="rear_left" output="screen">
        <rosparam>
            port: /dev/roboteq/rear-left
            baud: 115200
        </rosparam>
    </node>
    </group>
</launch>
