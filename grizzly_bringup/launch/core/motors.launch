<launch>
  <group ns="motors">
    <rosparam>
      front_right/driver:
        port: /dev/roboteq/front-right
        baud: 115200
      front_left/driver:
        port: /dev/roboteq/front-left
        baud: 115200
      rear_right/driver:
        port: /dev/roboteq/rear-right
        baud: 115200
      rear_left/driver:
        port: /dev/roboteq/rear-left
        baud: 115200
    </rosparam>
    <node pkg="roboteq_driver" type="driver_node" name="driver" ns="front_right" />
    <node pkg="roboteq_driver" type="driver_node" name="driver" ns="front_left" />
    <node pkg="roboteq_driver" type="driver_node" name="driver" ns="rear_right" />
    <node pkg="roboteq_driver" type="driver_node" name="driver" ns="rear_left" />
  </group>
</launch>
